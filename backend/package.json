{
  "name": "moneymind-backend",
  "version": "1.0.0",
  "description": "MoneyMind serverless backend - Personal finance tracker with intelligent categorization",
  "main": "src/api/handler.js",
  "type": "module",
  "scripts": {
    "dev": "serverless offline start",
    "test-server": "node test-server.js",
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "logs": "serverless logs -f api -t",
    "remove": "serverless remove",
    "test": "jest",
    "invoke:local": "serverless invoke local -f api"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.658.1",
    "@aws-sdk/lib-dynamodb": "^3.658.1",
    "aws-cdk": "^2.1024.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "express": "^4.18.2",
    "joi": "^17.13.3",
    "lambda-api": "^0.10.7",
    "serverless": "^3.40.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^24.1.0",
    "@types/uuid": "^10.0.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.55.1"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "personal-finance",
    "expense-tracker",
    "nodejs"
  ],
  "author": "MoneyMind Team",
  "license": "MIT"
}
